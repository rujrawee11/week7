      import json

pipeline {
    agent any

    stages {
        
        stage('Welcome to master'){
            steps{
                echo 'Start Program'
            }
        }

        stage('Tigger the Slave Jobs') {
            steps {
                script{
              
                with open('foo.txt') as f:
                variables = json.load(f)
               
                echo 'Start Trigger to send name and age to slave'
                build job: 'slave', parameters: [string(name: 'NAME', value: variables.name), string(name: 'Ages', value: '21')]
            
            }}
        }
    }
}
